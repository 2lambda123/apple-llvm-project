//--- temp_a.cpp
int bar(int x) {
	return x*x;
}
#include "template_function_header.h"
int a() {
  return foo<int>(2);
}

//--- template_function_header.h
template <typename T>
T foo(T s) {
  return s * s;
}

//--- temp_b.cpp
#include "template_function_header.h"
int b() {
  return foo<int>(2);
}

